// deploy to kubernetes

pipeline {
    agent docker
    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
                sh 'kubectl apply -f deployment.yaml'
            }
        }
    }
}
```